<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>PracticeServlet</display-name>
  <servlet>
    <servlet-name>com.app.core.Welcome</servlet-name>
    <servlet-class>com.app.core.Welcome</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>com.app.core.Validate</servlet-name>
    <servlet-class>com.app.core.Validate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.app.core.Welcome</servlet-name>
    <url-pattern>/servlet2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>com.app.core.Validate</servlet-name>
    <url-pattern>/servlet1</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>session.html</welcome-file>
  </welcome-file-list>
</web-app>